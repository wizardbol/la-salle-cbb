      },
      "ios": {
        "releaseChannel": "production",
        "image": "macos-sequoia-15.3-xcode-16.2",
        "distribution": "store",
        "autoIncrement": true,
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
